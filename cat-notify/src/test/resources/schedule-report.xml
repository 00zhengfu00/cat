<schedule-reports >
   <report domain="Cat" createClass="com.dianping.cat.notify.report.DailyReportCreater"  schedule="10"  >
      <template id="transaction" path="transaction.vm" />
      <template id="event" path="event.vm" />
	  <template id="problem" path="problem.vm" />
   </report>
   <report domain="Cat" createClass="com.dianping.cat.notify.report.WeaklyReportCreater"  schedule="10"  >
      <template id="transaction" path="weaklyTransaction.vm" />
      <template id="event" path="weaklyEvent.vm" />
	  <template id="problem" path="weaklyProblem.vm" />
   </report>
</schedule-reports>
