<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap>
	<typeAlias alias="mailLog" type="com.dianping.cat.notify.model.MailLog"/>
	<sql id="MailLog.columns">
		id,address,title,content,sendtime,status,error,cc
	</sql>
    <insert id="MailLog.insert"  parameterClass="mailLog">
    	insert into maillog (<include refid="MailLog.columns"/>) values
    	(#id#,#address#,#title#,#content#,now(),#status#,#error#,#cc#)
    </insert>
</sqlMap>
