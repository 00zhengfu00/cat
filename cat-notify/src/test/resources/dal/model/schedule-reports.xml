<schedule-reports >
   <default-reports exclude="Cat,mobileAPI">
         <template-report createClass="com.dianping.cat.notify.report.WeaklyReport" schedule="* * * * *">
		        <template id="transaction" path="/webapp/template/daily.vm" />
		        <template id="problem" path="/webapp/template/monthly.vm" />
		 </template-report>
		 <template-report createClass="com.dianping.cat.notify.report.DailyReport" schedule="* * * * *">
		 </template-report>
   </default-reports>
   <report domain="mobileAPI" createClass="com.dianping.cat.notify.report.WeaklyReport" schedule="* * * * *">
        <template id="transaction" path="/webapp/template/daily.vm" />
        <template id="problem" path="/webapp/template/monthly.vm" />
   </report>
   <report domain="mobileAPI" createClass="com.dianping.cat.notify.report.DailyReport" schedule="* * * * *">
   </report>
</schedule-reports>
