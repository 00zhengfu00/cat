<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">
	<!-- ===================== for processor ======================= -->
	<bean id="handler" class="com.dianping.cat.notify.server.StandardHandler" init-method="init">
		<property name="processors" >
		   <map>
		       <entry key="sendMail">
		          <ref bean="sendMailProccessor" />
		       </entry>
		   </map>
		</property>
	</bean>	
	<bean id="sendMailProccessor" class="com.dianping.cat.notify.server.processor.SendMailProccessor" />
</beans>