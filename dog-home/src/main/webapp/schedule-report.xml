<schedule-reports>
	<default-reports exclude="Cat">
		<template-report createClass="com.dianping.dog.notify.report.DailyReportCreater"
			schedule="10">
			<template id="transaction" path="transaction.vm" />
			<template id="event" path="event.vm" />
			<template id="problem" path="problem.vm" />
		</template-report>
		<template-report createClass="com.dianping.dog.notify.report.WeaklyReportCreater"
			schedule="10">
			<template id="transaction" path="weaklyTransaction.vm" />
			<template id="event" path="weaklyEvent.vm" />
			<template id="problem" path="weaklyProblem.vm" />
		</template-report>
	</default-reports>
	<!-- Cat Report Config -->
	<report domain="Cat"
		createClass="com.dianping.dog.notify.report.DailyReportCreater"
		schedule="10">
		<template id="transaction" path="transaction.vm" />
		<template id="event" path="event.vm" />
		<template id="problem" path="problem.vm" />
	</report>
	<report domain="Cat"
		createClass="com.dianping.dog.notify.report.WeaklyReportCreater"
		schedule="10">
		<template id="transaction" path="weaklyTransaction.vm" />
		<template id="event" path="weaklyEvent.vm" />
		<template id="problem" path="weaklyProblem.vm" />
	</report>
</schedule-reports>
