<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.dog.home.report.model" enable-merger="true" enable-json-builder="true"
	enable-xml-parser="true"  enable-sax-parser="true" enable-json-parser="true" enable-base-visitor="true">
	<entity name="schedule-reports" root="true">
    <entity-ref name="default-reports" />
    <entity-ref name="report" type="list" names="reports" />
  </entity>
  <entity name="default-reports">
    <attribute name="exclude" value-type="String" />
    <entity-ref name="template-report" type="list" names="template-reports" />
  </entity>
  <entity name="template-report">
    <attribute name="createClass" value-type="String" />
    <attribute name="schedule" value-type="String" />
    <entity-ref name="template" type="map" names="templates" />
  </entity>
  <entity name="template">
    <attribute name="id" key="true" value-type="String" />
    <attribute name="path" value-type="String" />
  </entity>
  <entity name="report">
    <attribute name="domain" value-type="String" />
    <attribute name="createClass" value-type="String" />
    <attribute name="schedule" value-type="String" />
    <entity-ref name="template" type="map" names="templates" />
  </entity>
</model>

