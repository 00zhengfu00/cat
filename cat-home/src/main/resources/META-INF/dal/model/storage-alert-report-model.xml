<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.cat.home.alert.report.storage" enable-merger="true"  enable-sax-parser="true"
	enable-base-visitor="true" enable-native-parser="true"  enable-native-builder="true">
  <entity name="storage-alert-report" root="true">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="start-time" value-type="Date" format="yyyy-MM-dd HH:mm:ss" />
    <attribute name="end-time" value-type="Date" format="yyyy-MM-dd HH:mm:ss" />
    <entity-ref name="segment" type="map" names="segments"  method-find-or-create="true" />
  </entity>
  <entity name="segment">
    <attribute name="id" value-type="int" key="true" primitive="true" />
    <entity-ref name="storage" type="map" names="storages"  method-find-or-create="true" />
  </entity>
  <entity name="storage">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="level" value-type="int" primitive="true" />
    <entity-ref name="machine" type="map" names="machines"  method-find-or-create="true" />
  </entity>
  <entity name="machine">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="level" value-type="int" primitive="true" />
    <entity-ref name="operation" type="map" names="operations"  method-find-or-create="true" />
  </entity>
  <entity name="operation">
    <attribute name="id" value-type="String" key="true" />
    <attribute name="level" value-type="int" primitive="true" />
    <entity-ref name="attribute" type="map" names="attributes"  method-find-or-create="true" />
  </entity>
<entity name="attribute">
    <attribute name="id" value-type="String" key="true"/>
    <attribute name="title" value-type="String" />
    <attribute name="level" value-type="int" primitive="true" />
    <entity-ref name="detail" type="list" names="details" />
  </entity>
  <entity name="detail">
    <attribute name="content" value-type="String" key="true" />
    <attribute name="level" value-type="int" primitive="true" />
  </entity>
</model>

