<?xml version="1.0" encoding="utf-8"?>
<display-policy>
   <group id="HeapUsage" order="1">
      <metric id="EdenUsage" unit="M" delta="false" order="1" alert="true"/>
      <metric id="SurvivorUsage" unit="M" delta="false" order="2" alert="true"/>
      <metric id="CodeCache" unit="M" delta="false" order="3" alert="false"/>
      <metric id="ParEdenSpace" unit="M" delta="false" order="4" alert="false"/>
      <metric id="ParSurvivorSpace" unit="M" delta="false" order="5" alert="false"/>
      <metric id="CMSOldGen" unit="M" delta="false" order="6" alert="false"/>
      <metric id="CMSPermGen" unit="M" delta="false" order="7" alert="false"/>
   </group>
   <group id="default">
      <metric id="TotalStartedCount" delta="true" alert="true"/>
      <metric id="NewGcCount" delta="true" alert="true"/>
      <metric id="OldGcCount" delta="true" alert="true"/>
      <metric id="CatMessageSize" delta="true" alert="false"/>
      <metric id="CatMessageOverflow" delta="true" alert="false"/>
   </group>
</display-policy>
