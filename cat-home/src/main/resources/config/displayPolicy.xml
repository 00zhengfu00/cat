<display-policy>
	<group id="HeapUsage" order="1">
		<metric id="EdenUsage" unit="M" delta="false" order="1"/>
		<metric id="SurvivorUsage" unit="M" delta="false" order="2"/>
	</group>
	<group id="default">
		<metric id="TotalStartedCount" delta="true"/>
		<metric id="NewGcCount"  delta="true"/>
		<metric id="OldGcCount"  delta="true"/>
		<metric id="CatMessageSize"  delta="true"/>
		<metric id="CatMessageOverflow"  delta="true"/>
	</group>
</display-policy>