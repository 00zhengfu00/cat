<monitor-rules>
   <rule id="ErrorAndDiscard">
      <metric-item type="regex" monitorSum="true" text="^(?!(2400-1-dianping-com|SH-N6001P-1|SH-N6001P-2)).*(?=errors$)"/>
      <metric-item type="regex" monitorSum="true" text=".*(?=discards$)"/>
      <config starttime="00:00" endtime="24:00">
         <condition minute="3">
            <sub-condition type="MaxVal" text="400"/>
         </condition>
      </config>
   </rule>
