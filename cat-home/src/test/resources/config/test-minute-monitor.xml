<monitor-rules>
	<rule id="two-minute">
		<config starttime="00:00" endtime="24:00">
			<condition minute="2">
					<sub-condition type="DescPer">40</sub-condition>
			</condition>
			<condition minute="2">
					<sub-condition type="DescVal">80</sub-condition>
			</condition>
		</config>
	</rule>
</monitor-rules>
